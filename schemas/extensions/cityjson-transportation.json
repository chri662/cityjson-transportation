{
   "$schema":"http://json-schema.org/schema#",
   "type":"object",
   "$id":"extensiontranspotation.schema.json",
   "uri":"https://github.com/chri662/cityjson-transportation/edit/main/schemas/extensions/cityjson-transportation.json",
   "version":"0.1",
   "description":"Extension of transport module. Works with CityJSON 1.0.1",
   "extraCityObjects":{
      "+GranularityValue":{
         "allOf":[
            {
               "$ref":"#/_AbstractTransportationSpace"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "lane",
                        "way"
                     ]
                  }
               },
               "required":[
                  "type"
               ]
            }
         ]
      },
      "+TrafficDirectionValue":{
         "allOf":[
            {
               "$ref":"#/_AbstractTransportationSpace"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "forwards",
                        "backwards",
                        "both"
                     ]
                  }
               },
               "required":[
                  "type"
               ]
            }
         ]
      },
      "+TrafficSpace":{
         "allOf":[
            {
               "$ref":"#/_AbstractUnoccupiedSpace"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "TrafficSpace"
                     ]
                  },
                  "granularity":{
                     
                  },
                  "trafficDirection":{
                     
                  },
                  "Occupacy":{
                     
                  },
                  "predecessor":{
                     
                  },
                  "successor":{
                     
                  }
               },
               "required":[
                  "type",
                  "granularity",
                  "trafficDirection",
                  "Occupacy",
                  "predecessor",
                  "successor"
               ]
            }
         ]
      },
      "+ClearanceSpace":{
         "allOf":[
            {
               "$ref":"#/_AbstractUnoccupiedSpace"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "ClearanceSpace"
                     ],
                     "parent":{
                        "type":"string",
                        "description":"the ID of the children TrafficSpace"
                     }
                  }
               },
               "required":[
                  "type",
                  "parent"
               ]
            }
         ]
      },
      "+AuxiliaryTrafficSpace":{
         "allOf":[
            {
               "$ref":"#/_AbstractUnoccupiedSpace"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "AuxiliaryTrafficSpace"
                     ]
                  },
                  "granularity":{
                     
                  }
               },
               "required":[
                  "type",
                  "granularity"
               ]
            }
         ]
      },
      "+Marking":{
         "allOf":[
            {
               "$ref":"#/_AbstractThematicSurface"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "Marking"
                     ]
                  }
               },
               "required":[
                  "type"
               ]
            }
         ]
      },
      "+Hole":{
         "allOf":[
            {
               "$ref":"#/_AbstractThematicSurface"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "Hole"
                     ]
                  },
                  "parent":{
                     "type":"string",
                     "description":"the ID of the children Marking"
                  },
                  "required":[
                     "type"
                  ]
               }
            }
         ]
      },
      "+waterway":{
         "allOf":[
            {
               "$ref":"#/_AbstractTransportationSpace"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "waterway"
                     ]
                  }
               },
               "required":[
                  "type"
               ]
            }
         ]
      },
      "+section":{
         "allOf":[
            {
               "$ref":"#/_AbstractTransportationComplex"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "section"
                     ],
                     "parent":{
                        "type":"string",
                        "description":"the ID of the children of track"
                     }
                  }
               },
               "required":[
                  "type",
                  "parent"
               ]
            }
         ]
      },
      "+intersection":{
         "allOf":[
            {
               "$ref":"#/_AbstractTransportationComplex"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "intersection"
                     ],
                     "parent":{
                        "type":"string",
                        "description":"the ID of the children of track"
                     }
                  }
               },
               "required":[
                  "type",
                  "parent"
               ]
            }
         ]
      },
      "+HoleSuface":{
         "allOf":[
            {
               "$ref":"#/_AbstractThematicSurface"
            },
            {
               "properties":{
                  "type":{
                     "enum":[
                        "HoleSuface"
                     ]
                  }
               },
               "required":[
                  "type"
               ]
            }
         ]
      }
   },
   "additionalProperties":{
      "TrafficArea":{
         "allOf":[
            {
               "$ref":"#/_AbstractTransportationComplex"
            },
            {
               "properties":{
                  "+HoleSuface":{
                     
                  },
                  "+boundary":{
                     
                  }
               }
            }
         ]
      },
      "AuxiliaryTrafficArea":{
         "allOf":[
            {
               "$ref":"#/_AbstractTransportationComplex"
            },
            {
               "properties":{
                  "+boundary":{
                     
                  }
               }
            }
         ]
      }
   }
}


